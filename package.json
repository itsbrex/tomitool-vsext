{
  "name": "tomitool",
  "displayName": "tomitool",
  "description": "Tomitool to add custom syntax hilight",
  "version": "0.0.7",
  "repository": {
    "type": "git",
    "url": "https://github.com/tomimick/tomitool-vsext"
  },
  "engines": {
    "vscode": "^1.63.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:tomitool.helloWorld"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "tomitool.helloWorld",
        "title": "Tomitool.Hello World"
      }
    ],
    "grammars": [
      {
        "language": "plaintext",
        "scopeName": "text.tomiscope",
        "path": "./syntaxes/tomitext.tmLanguage.json"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.63.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "eslint": "^8.6.0",
    "glob": "^7.2.0",
    "mocha": "^9.1.3",
    "typescript": "^4.5.4",
    "@vscode/test-electron": "^2.0.3"
  }
}
